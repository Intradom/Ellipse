            <string>// Spawn enemy if possible
if (can_spawn)
{
    var spawn_x;
    var spawn_y;
    var spawn_dir = irandom(3);
    var enemy_rad = sprite_get_width(sp_enemy_absorb) / 2.0 * global.ENEMY_SCALING_BASE;
    if (spawn_dir == 0) // up
    {
        spawn_x = enemy_rad + (room_width - enemy_rad) * random(1);
        spawn_y = enemy_rad;
    }
    if (spawn_dir == 1) // down
    {
        spawn_x = enemy_rad + (room_width - enemy_rad) * random(1);
        spawn_y = room_height - enemy_rad;
    }
    if (spawn_dir == 2) // left
    {
        spawn_x = enemy_rad;
        spawn_y = enemy_rad + (room_height - enemy_rad) * random(1);
    }
    if (spawn_dir == 3) // right
    {
        spawn_x = room_width - enemy_rad;
        spawn_y = enemy_rad + (room_height - enemy_rad) * random(1);
    }
    
    var enemy_id = instance_create(spawn_x, spawn_y, obj_enemy_absorb);
    enemy_id.image_xscale *= global.ENEMY_SCALING_BASE;
    enemy_id.image_yscale *= global.ENEMY_SCALING_BASE;
       
    can_spawn = false;
    alarm[0] = room_speed * global.SPAWN_TIMER;
}
</string>
